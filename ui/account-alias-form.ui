<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.14"/>
  <template class="AccountAliasForm" parent="AccountForm">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="orientation">vertical</property>
    <style>
      <class name="geary-common-preferences-dialog-container"/>
    </style>
    <child>
      <object class="GtkFrame" id="category_general">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <style>
          <class name="geary-common-preferences-category"/>
        </style>
        <child>
          <object class="GtkGrid" id="category_general_grid">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="row_spacing">6</property>
            <child>
              <object class="GtkLabel" id="name_label">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="halign">start</property>
                <property name="label" translatable="yes">Your name</property>
              </object>
              <packing>
                <property name="top_attach">0</property>
                <property name="left_attach">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkEntry" id="name_value">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
              </object>
              <packing>
                <property name="top_attach">0</property>
                <property name="left_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="email_label">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="halign">start</property>
                <property name="label" translatable="yes">Email address</property>
              </object>
              <packing>
                <property name="top_attach">1</property>
                <property name="left_attach">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkEntry" id="email_value">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="width_chars">35</property>
                <property name="input_purpose">email</property>
              </object>
              <packing>
                <property name="top_attach">1</property>
                <property name="left_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="email_validation_label">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="use_markup">True</property>
              </object>
              <packing>
                <property name="top_attach">1</property>
                <property name="left_attach">2</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="make_primary_button">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="halign">start</property>
                <property name="label" translatable="yes">Make primary</property>
              </object>
              <packing>
                <property name="top_attach">2</property>
                <property name="left_attach">0</property>
                <property name="width">2</property>
              </packing>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
